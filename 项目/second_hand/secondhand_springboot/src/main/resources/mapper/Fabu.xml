<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.secondhand_springboot.mapper.FabuMapper">
    <insert id="save">
        insert into goods(g_name,c_id,price,`describe`,sub_time,g_state,image,login_id)
        values(#{g_name},#{c_id},#{price},#{describe},#{sub_time},#{g_state},#{image},#{login_id})
    </insert>
</mapper>
